{
  "name": "vitdoc-monorepo",
  "version": "2.0.1",
  "description": "Seamlessly generate a lightweight, fast and beautiful component documentation.",
  "private": true,
  "scripts": {
    "dev": "turbo --filter='./templates/*' --filter='./packages/*' dev",
    "example:dev": "turbo --filter='./examples/*' dev",
    "test": "vitest run",
    "prettier": "pretty-quick --staged",
    "build": "turbo --filter='./templates/*' --filter='./packages/*' build",
    "release": "bumpp package.json packages/**/package.json && pnpm -r --no-git-checks publish --access public"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitdocjs/vitdoc.git"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "bumpp": "^8.2.1",
    "esbuild": "^0.14.54",
    "esbuild-sass-plugin": "^2.4.1",
    "father": "^4.1.0",
    "turbo": "^1.6.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  }
}
