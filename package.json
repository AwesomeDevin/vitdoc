{
  "name": "vitdoc-monorepo",
  "version": "1.0.14",
  "description": "Seamlessly generate a lightweight, fast and beautiful component documentation.",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --no-sort --filter='./templates/*' --filter='./packages/*' dev",
    "example:dev": "pnpm -r --filter='./examples/*' dev",
    "test": "vitest run",
    "prettier": "pretty-quick --staged",
    "build": "pnpm -r run build",
    "release": "bumpp package.json packages/**/package.json",
    "prepublishOnly": "npm run test && npm run build"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitdocjs/vitdoc.git"
  },
  "devDependencies": {
    "father": "^4.1.0",
    "vite": "^3.0.8"
  }
}
